local function HopServer()
	config = {
		maxPlayers = 10,
		minPlayers = 1,
		retryDelay = 3,
		maxRetries = 20
	}
	services = {
		TeleportService = game:GetService("TeleportService"),
		Players = game:GetService("Players")
	}
	attempts = 0
	print("[HopServer] ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤ Server ‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏Ñ‡∏ô " .. config.minPlayers .. "-" .. config.maxPlayers .. " ‡∏Ñ‡∏ô")
	while attempts < config.maxRetries do
		attempts = attempts + 1
		print("[HopServer] ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏û‡∏¢‡∏≤‡∏¢‡∏≤‡∏°‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà " .. attempts)
		local success, serverList = pcall(function()
			return services.TeleportService:GetGameInstances(Lobby_Id, "", 50)
		end)
		if success and serverList then
			local validServers = {}
			for _, server in ipairs(serverList.Collection) do
				if server.Playing >= config.minPlayers and server.Playing <= config.maxPlayers then
					table.insert(validServers, {
						id = server.Guid,
						players = server.Playing,
						maxPlayers = server.MaxPlayers
					})
				end
			end
			if #validServers > 0 then
				table.sort(validServers, function(a, b)
					return a.players < b.players
				end)
				local bestServer = validServers[1]
				print("[HopServer] ‡∏û‡∏ö Server: " .. bestServer.players .. "/" .. bestServer.maxPlayers .. " ‡∏Ñ‡∏ô")
				local teleportSuccess = pcall(function()
					services.TeleportService:TeleportToPlaceInstance(Lobby_Id, bestServer.id, services.Players.LocalPlayer)
				end)
				if teleportSuccess then
					print("[HopServer] ‡∏Å‡∏≥‡∏•‡∏±‡∏á Teleport...")
					break
				else
					print("[HopServer] Teleport ‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß")
				end
			else
				print("[HopServer] ‡πÑ‡∏°‡πà‡∏û‡∏ö Server ‡∏ó‡∏µ‡πà‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°")
			end
		else
			print("[HopServer] ‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏î‡∏∂‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏• Server ‡πÑ‡∏î‡πâ")
		end
		if attempts < config.maxRetries then
			wait(config.retryDelay)
		end
	end
	if attempts >= config.maxRetries then
		print("[HopServer] ‡∏´‡∏≤ Server ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å " .. config.maxRetries .. " ‡∏Ñ‡∏£‡∏±‡πâ‡∏á")
	end
end

repeat
	wait(0.1)
	if repeat_stuc_count < 3 then
		repeat_stuc_count = repeat_stuc_count + 0.1
	else
		wait(1)
		HopServer()
	end
until game.Players.LocalPlayer.Character

